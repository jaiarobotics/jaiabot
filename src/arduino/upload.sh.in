#!/bin/bash

sharedir=../../
sketch=@sketchname@
hex=@hex@
nickname=@nickname@
baud_flag=@baudrate_flag@
port_flag=@serial_port_flag@

avrdude -patmega328p -C+${sharedir}/avrdude.conf -carduino -Uflash:w:${sharedir}/${sketch}/${nickname}/${hex}:i ${baud_flag} ${port_flag} -v -D -V || echo "UPLOAD FAILED!"
