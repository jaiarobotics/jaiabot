$app_block
$interprocess_block

bot_id: $bot_id

command_sub_cfg {
    intervehicle
    {
        # hub modem id
        publisher_id: 1

        # buffer configuration for the Command messages
        buffer {
            ack_required: false
            blackout_time: 0
            max_queue: 1
            newest_first: true
            ttl: 600
            value_base: 1000
        }
    }
}

use_rpm_table_for_speed: true
rpm_table { speed: 0 rpm: 0 }
rpm_table { speed: 1 rpm: 25 }
rpm_table { speed: 2 rpm: 50 }
rpm_table { speed: 3 rpm: 75 }
rpm_table { speed: 4 rpm: 100 }
