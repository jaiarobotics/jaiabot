$app_block
$interprocess_block

bot_id: $bot_id

use_throttle_table_for_speed: true

### Do not change the throttle table unless
### You know the consequences (May result in bot hardware failure)
### The speed value (m/s) is updated in the UI (Wrench button)
throttle_table { speed: 0 throttle: 0 }
throttle_table { speed: 5 throttle: 100 }

### Constant to add to dive throttle
### Negative value will increase dive throttle
### BEAWARE: only change in small increments
dive_throttle_addition: 0 # default = 0 

$jaiabot_driver_arduino_bounds
