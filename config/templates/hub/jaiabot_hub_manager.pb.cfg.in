$app_block
$interprocess_block

fleet_id: $fleet_id
hub_id: $hub_id

status_sub_cfg {
    intervehicle {
        # publisher_id added in code for each configured managed_bot_modem_id
        # buffer configuration for BotStatus messages
        buffer {
            ack_required: false
            blackout_time: 1
            max_queue: 1 
            newest_first: true 
            ttl: 60
            value_base: 10
        }
    }
}

engineering_status_sub_cfg {
    intervehicle {
        # publisher_id added in code for each configured managed_bot_modem_id
        # buffer configuration for EngineeringStatus messages
        buffer {
            ack_required: false
            blackout_time: 1
            max_queue: 1 
            newest_first: true 
            ttl: 30
            value_base: 10
        }
    }
}

task_packet_sub_cfg {
    intervehicle {
        # publisher_id added in code for each configured managed_bot_modem_id
        # buffer configuration for TaskPacket messages
        buffer {
            ack_required: true
            blackout_time: 0
            max_queue: 42
            newest_first: false
            ttl: 3000
            value_base: 5
        }
    }
}

xbee {
    $xbee_config
}

hub_gpsd_device: "$hub_gpsd_device"

# Feed bot ID 1 as contact 0 via GPSD
bot_to_gps  { bot_id: 1   udp { remote_address: "127.0.0.1" remote_port: 33001 }}
bot_to_gps  { bot_id: 2   udp { remote_address: "127.0.0.1" remote_port: 33002 }}
bot_to_gps  { bot_id: 3   udp { remote_address: "127.0.0.1" remote_port: 33003 }}
contact_gps { contact: 0  gpsd_device: "udp://127.0.0.1:33001" }
