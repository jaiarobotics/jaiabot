link {
    modem_id: $modem_id
    subnet_mask: $subnet_mask
    driver {
        driver_type: DRIVER_UDP
        [goby.acomms.udp.protobuf.config] { 
            local {                   
                port: $local_port
            }
            $remotes
            max_frame_size: 1400
        }
    }
    mac { 
        type: MAC_FIXED_DECENTRALIZED 
        slot { src: $modem_id type: DATA slot_seconds: 0.5 }
    }

    # read from _sub_buffer.pb.cfg.in
    $sub_buffer
}
