[Unit]
Description=$description

BindsTo=jaiabot_gobyd.service
After=jaiabot_gobyd.service
$extra_unit

[Service]
EnvironmentFile=$env_file
# Run python code within a pre-installed venv
ExecStart=/bin/bash -c ". $jaiabot_share_dir/jaiabot-python/venv/bin/activate; $jaiabot_share_dir/jaiabot-python/$subdir/$exe $args"
$extra_service

[Install]
WantedBy=jaiabot_gobyd.service
